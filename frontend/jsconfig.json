{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "allowJs": true,
    "checkJs": false,
    "jsx": "preserve",
    //"moduleResolution": "node",
    "module": "preserve",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
